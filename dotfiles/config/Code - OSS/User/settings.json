{
    "window.title": "Code",
    "workbench.colorTheme": "vs-wal",
    "workbench.statusBar.visible": true,
    "git.confirmSync": false,
    "breadcrumbs.filePath": "off",
    "git.autofetch": true,
    "files.associations": {
        "*.mu": "markdown",
        "*.pyx": "python",
        "*.gin2": "properties",
        "*.gscn": "properties",
        "*.tmpl": "plaintext"
    },
    "editor.quickSuggestions": {
        "other": true,
    },
    "cSpell.userWords": [
        "Precourt",
        "Prelab",
        "mathf",
        "multicols"
    ],
    "window.closeWhenEmpty": false,
    "files.hotExit": "off",
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "window.menuBarVisibility": "default","themeGenerator.colors": {
        "background": "#21282d",
        "foreground": "#fff",
        "primary": "#327193",
        "keywords": "#43baba",
        "functions": "#3fa5c1",
        "strings": "#b9ecfa",
        "numbers": "#8789c0",
        "punctuation": "#494f53",
        "operators": "#327193",
        "comment": "#3d4b54",
        "tags": "#43baba",
        "attributes": "#3fa5c1",
        "properties": "#43baba",
        "builtins": "#b5ca8d",
        "variables": "#fff",
        "types": "#3fa5c1"
    },
    "workbench.iconTheme": "simple-icons",
    "dashboard.projectData": null,
    "editor.fontWeight": "bold",
    "travisClient.branches": [
        "master",
        "future"
    ],
    "editor.minimap.renderCharacters": false,
    "editor.fontSize": 16,
    "cSpell.enableFiletypes": [
        "!c",
        "!cpp",
        "!csharp"
    ],
    "files.exclude": {
        "**/.vscode": true
    },
    "editor.tabSize": 2,
    "workbench.editorAssociations": {
        "*.bin": "hexEditor.hexedit"
    },
    "zenMode.fullScreen": false,
    "zenMode.hideTabs": false,
    "[jsonc]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "editor.detectIndentation": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "workbench.list.smoothScrolling": true,
    "omnisharp.organizeImportsOnFormat": true,
    "redhat.telemetry.enabled": true,
    "tabnine.experimentalAutoImports": true,
    "todo-tree.general.statusBar": "top three",
    "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
    "todo-tree.general.statusBarClickBehaviour": "cycle",
    "editor.cursorBlinking": "phase",
    "[csharp]": {
      "editor.tabSize": 4,
    },
    "markdown.preview.breaks": true,
    "kite.showWelcomeNotificationOnStartup": false,
    "editor.fontFamily": "'CaskaydiaCovePL Nerd Font'",
    "editor.suggest.showReferences": false,
    "task.autoDetect": "off",
    "nim.enableNimsuggest": false,
    "nim.provider": "lsp",
    
}